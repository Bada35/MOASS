pipeline {
    agent any

    stages {
        stage('Embedded Build') {
	        steps {
				dir('Embedded/electron-app') {
                    sh "docker build --no-cache -t dongho416/embedded:${currentBuild.number} ."
                    sh "docker push dongho416/embedded:${currentBuild.number}"
                    sh "docker rmi dongho416/embedded:${currentBuild.number}"
                }
            }
        }
    }
}
